{
  "name": "stylex",
  "version": "0.1.1",
  "license": "MIT",
  "description": "Easily convert CSS styles between the DOM and Javascript, and back.",
  "repository": "ryanhefner/stylex",
  "bugs": "https://github.com/ryanhefner/stylex/issues",
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "files": [
    "index.js",
    "es",
    "src",
    "umd"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -f index.js && rm -rf es && rm -rf umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js",
    "watch": "babel ./src -d . --ignore __tests__ --watch",
    "prepare": "npm run build",
    "prepublishOnly": "node ./tools/build.js",
    "push-release": "git push origin master && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "gzip-size": "^4.0.0",
    "pretty-bytes": "^4.0.2",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "uglify-es": "^3.1.0"
  },
  "keywords": [
    "css",
    "javascript",
    "dom",
    "convert",
    "conversion",
    "transpiler"
  ]
}
